{
  "name": "tree-vue",
  "description": "Treeview component for Vue.js",
  "version": "0.1.0",
  "author": "Anatoliy Ivanov <t0lik@t0lik.com>",
  "library": "Treevue",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/tree-vue.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/tree-vue.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/tree-vue.min.js",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build-simple": "vue-cli-service build --target lib --name tree-vue src/components/TreeVue.vue",
    "build-app": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --reporter min",
    "lint": "vue-cli-service lint"
  },
  "main": "dist/tree-vue.umd.js",
  "module": "dist/tree-vue.esm.js",
  "unpkg": "dist/tree-vue.min.js",
  "browser": {
    "./sfc": "src/components/TreeVue.vue"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t0lik/tree-vue.git"
  },
  "homepage": "https://github.com/t0lik/tree-vue#readme",
  "license": "MIT",
  "keywords": [
    "vue",
    "tree",
    "treeview",
    "component"
  ],
  "dependencies": {},
  "files": [
    "src",
    "dist/*.js"
  ],
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-plugin-unit-mocha": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "core-js": "^3.4.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.0",
    "rollup": "^1.27.0",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-vue": "^5.1.2",
    "sinon": "^7.5.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
